{
  "name": "mqtt-analyser",
  "version": "0.0.0",
  "description": "Analyses the performance of an MQTT Broker :rocket:",
  "author": "Tim James",
  "homepage": "https://github.com/Tim-W-James/mqtt-analyser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tim-W-James/mqtt-analyser.git"
  },
  "keywords": [
    "project_keywords"
  ],
  "bugs": {
    "url": "https://github.com/Tim-W-James/mqtt-analyser/issues"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "scripts": {
    "dev": "nodemon --watch './**/*.ts' --watch '.env' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "build": "tsc --project tsconfig.build.json",
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitest/ui": "^0.10.5",
    "c8": "^7.11.2",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-vitest": "^0.0.8",
    "jsdom": "^19.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vitest": "^0.10.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.4",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "mqtt": "^4.3.7",
    "nanospinner": "^1.1.0",
    "readonly-types": "^3.0.0",
    "source-map-support": "^0.5.21"
  }
}
